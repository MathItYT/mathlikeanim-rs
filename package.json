{
    "name": "mathlikeanim-rs",
    "description": "A Rust library for creating mathematical animations",
    "version": "0.7.21",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MathItYT/mathlikeanim-rs"
    },
    "files": [
        "javascript/browser/mathlikeanim_rs_bg.wasm",
        "javascript/browser/mathlikeanim_rs.js",
        "javascript/browser/mathlikeanim_rs.d.ts",
        "javascript/browser/mathlikeanim_rs_bg.wasm.d.ts",
        "javascript/node/mathlikeanim_rs_bg.wasm",
        "javascript/node/mathlikeanim_rs_bg.wasm.d.ts",
        "javascript/node/mathlikeanim_rs.js",
        "javascript/node/mathlikeanim_rs.d.ts"
    ],
    "exports": {
      "./node": {
        "require": "./javascript/node/mathlikeanim_rs.js",
        "types": "./javascript/node/mathlikeanim_rs.d.ts"
      },
      "./browser": {
        "import": "./javascript/browser/mathlikeanim_rs.js",
        "types": "./javascript/browser/mathlikeanim_rs.d.ts"
      }
    },
    "sideEffects": [
        "./snippets/*"
    ],
    "keywords": [
        "animation",
        "graphics",
        "easing",
        "transitions",
        "WebAssembly"
    ],
    "dependencies": {
        "@types/fluent-ffmpeg": "^2.1.24",
        "canvas": "^2.11.2",
        "fluent-ffmpeg": "^2.1.3",
        "mathjax-full": "^3.2.2",
        "opentype.js": "^1.3.4"
    }
}
